<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:int="http://www.springframework.org/schema/integration"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/integration
		http://www.springframework.org/schema/integration/spring-integration-2.1.xsd">

	<bean id="reseller" class="edu.integration.patterns.reseller.ResellerImpl" />
	<bean id="producer" class="edu.integration.patterns.producer.ProducerImpl" />
	<bean id="producerListener" class="edu.integration.patterns.producer.ProducerListener">
		<constructor-arg ref="producer" />
		<constructor-arg ref="channel" />
	</bean>

	<int:channel id="channel">
		<int:queue />
	</int:channel>


</beans>
